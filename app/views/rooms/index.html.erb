<div>
  <h3>管理している部屋</h3>
  <ul>
    <% current_user.rooms.each do |room| %>
      <li><%= link_to room.name, room_path(room), data: { turbolinks: false } %> <%= link_to "削除", room_path(room), method: :delete %></li>
    <% end %>
  </ul>
  <h3>すべての部屋</h3>
  <ul>
    <% @rooms.each do |room| %>
      <li><%= link_to room.name, room_path(room), data: { turbolinks: false } %></li>
    <% end %>
  </ul>
</div>
